{
	"manifest_version": 2,
	"default_locale": "en",
	"background": {
		"scripts": [ "background.js" ],
		"persistent": true
	},
	"name": "Tree Tabs",
	"description": "__MSG_extDesc__",
	"icons": {
		"128": "icons/128.png",
		"48": "icons/48.png",
		"38": "icons/24.png",
		"19": "icons/16.png",
		"16": "icons/16.png"
	},
	"permissions": [ "tabs", "sessions", "<all_urls>" ],
	"commands": {
		"right_tree_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Right"
			},
			"description": "Go to next sibling tab"
		},
		"left_tree_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Left"
			},
			"description": "Go to previous sibling tab"
		},
		"up_tree_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Up"
			},
			"description": "Go to parent tab"
		},
		"down_tree_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Down"
			},
			"description": "Go to first child tab"
		},
		"move_right_tree_tab": {
			"suggested_key": {
				"default": "Alt+Shift+Right"
			},
			"description": "Go to next sibling tab"
		},
		"move_left_tree_tab": {
			"suggested_key": {
				"default": "Alt+Shift+Left"
			},
			"description": "Go to previous sibling tab"
		},
		"move_up_tree_tab": {
			"suggested_key": {
				"default": "Alt+Shift+Up"
			},
			"description": "Go to parent tab"
		},
		"move_down_tree_tab": {
			"suggested_key": {
				"default": "Alt+Shift+Down"
			},
			"description": "Go to first child tab"
		}
	},
	"sidebar_action": {
		"default_icon": {
			"19": "icons/16.png",
			"38": "icons/38.png"
		},
		"default_panel": "sidebar.html",
		"default_title": "Tree Tabs"
	},
	"options_page": "options.html",
	"applications": {
		"gecko": {
			"id": "TreeTabs@jagiello.it",
			"strict_min_version": "54.0"
		}
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab":true
	},
	"version": "0.0.16"
}
